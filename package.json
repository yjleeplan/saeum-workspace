{
  "name": "saeum-workspace",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.local vite",
    "dev": "vite",
    "preview": "vite preview",
    "build:production": "env-cmd -f .env.production vite build",
    "build:development": "env-cmd -f .env.development vite build --mode development"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@tanstack/query-core": "^4.32.0",
