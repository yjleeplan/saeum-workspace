    "@tanstack/react-query-devtools": "^4.32.0",
    "ag-grid-community": "^26.2.1",
    "ag-grid-react": "^26.2.0",
    "antd": "^4.8.0",
    "axios": "^1.9.0",
    "env-cmd": "^10.1.0",
    "immer": "^10.1.1",
    "moment": "^2.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-player": "^2.9.0",
    "react-router-dom": "^6.14.2",
    "styled-components": "^6.1.19",
