{
  "name": "saeum-workspace",
  "version": "0.1.0",
  "repository": "https://github.com/yjleeplan/saeum-workspace.git",
  "private": true,
  "scripts": {
    "dev": "cp-cli ./config/.env.dev ./.env",
    "staging": "cp-cli ./config/.env.staging ./.env",
    "prod": "cp-cli ./config/.env.prod ./.env",
    "build": "npm run sync-assets && react-scripts build",
    "build-dev": "npm run dev && npm run build",
    "build-staging": "npm run staging && npm run build",
    "build-prod": "npm run prod && npm run build",
    "start": "react-scripts start",
    "start-dev": "npm run dev && npm-run-all -p sync-assets watch-assets start",
    "start-staging": "npm run staging && npm-run-all -p sync-assets watch-assets start",
    "start-prod": "npm run prod && npm-run-all -p sync-assets watch-assets start",
    "sync-assets": "cp -r ./src/assets/ ./public/assets",
    "watch-assets": "chokidar \"src/assets\" -c \"npm run sync-assets\""
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "ag-grid-community": "^26.2.1",
    "ag-grid-react": "^26.2.0",
    "antd": "^4.8.0",
    "axios": "^0.21.0",
    "chokidar-cli": "^2.1.0",
    "cp-cli": "^2.0.0",
    "crypto": "^1.0.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^0.2.4",
    "xlsx": "^0.18.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
